package com.sorting.algo;

public class SelectionSort {
	
	public  void selection_sort(int[] arr){
		
		for(int i=0; i<arr.length-1;i++)
		{
			int min=arr[0];
			for(int j=i+1;j<arr.length;j++)
				{
					if(arr[j]<arr[min])
					{
						
					}
				}
		}
		
	}
	
	public static void main(String[] args) {
		
	}

}
