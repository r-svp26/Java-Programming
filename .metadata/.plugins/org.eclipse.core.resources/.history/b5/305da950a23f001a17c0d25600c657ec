package com.search.algo;

public class LinearSearch {

	public static int linear_search(int[] arr, int data) {
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] == data) {
				return i;
			}
		}
		return -1;
	}

	public static void main(String[] args) {

		int[] arr = { 10, 23, 25, 15, 26 };
		int data = 5;
		System.out.println("Element found at index:" + (LinearSearch.linear_search(arr, data)));
	}
}
