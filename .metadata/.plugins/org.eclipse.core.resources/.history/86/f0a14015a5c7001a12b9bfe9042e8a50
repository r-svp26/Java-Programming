package com.string.interview;

public class AnagramString {
	
	public static boolean isAnagram(String s1,String s2){
		
		int[] a=new int[26];
		for(int i=0;i<s1.length();i++){
			char ch=s1.charAt(i);
			if(ch>='A'&& ch<='Z')
				a[ch-65]++;
			else if(ch>'a'&& ch<='z')
				a[ch-97]++;
			
		}
		
		return true;
	}

	public static void main(String[] args) {
		
	}

}
