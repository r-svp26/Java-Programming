package org.thread.in;

public class MainThread5 {

	public static void main(String[] args) {
		
		Thread t1=new Thread(){
			
			public void run(){
				for(int i=0;i<=10;i++){
					System.out.println("2*"+i+"="+2*i);
					
					try {
						Thread.sleep(300);
					} catch (InterruptedException e) {
						e.printStackTrace();
					}
				}
			}
		};
	}
}
