package org.thread.in;

public class SimpleTest {

	public static void main(String[] args) {
		
		SimpleThread sthread=new SimpleThread();
		
		@SuppressWarnings("unused")
		class Thread1 extends Thread{
			@Override
			public void run(){
				sthread.display();
			}
		}
		
		@SuppressWarnings("unused")
		class Thread2 extends Thread{
			@Override
			public void run() {
				sthread.print();
			}
			
		}
		
	}
}
