package org.thread.in;

public class MainThread7 extends Thread{
	
	public MainThread7(String name){
		super(name);	
	}
	
	public MainThread7(){
		
	}
	
	public void run(){
		for(int i=1;i<=10;i++){
			System.out.println(i+"<=>"+Thread.currentThread().getName());
			try {
				Thread.sleep(200);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}
}
