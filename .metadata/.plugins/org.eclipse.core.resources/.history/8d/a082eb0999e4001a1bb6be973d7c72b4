package org.serialization.in;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.IOException;

/* WAJP to Copy Same Program Content( Source Code ) to another File */

public class SourceCopied {

	public static void main(String[] args) throws IOException {
		
		System.out.println("Main method Started...");
		
		FileInputStream fin=new FileInputStream("V:/Programming/FileHandling/src/org/serialization/in/SourceCopied");
		
		int i, count=0;
		
		while((i=fin.read())!=-1){
			System.out.println((char)i);
			if(i=='\n')
				count++;
		}

	}
}
