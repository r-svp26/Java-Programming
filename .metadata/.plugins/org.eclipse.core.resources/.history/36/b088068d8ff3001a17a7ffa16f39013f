package com.softxyz;

import java.util.Scanner;

public class BinaryString {

	static int countStep(String str1) {
		int count = 0;
		String str2="";

		char[] ch = str1.toCharArray();
		for (int i = 0; i < ch.length; i++) {
			int j=i;
			while(ch[i]!=' ')
				i++;
			
				int k=i-1;
				char temp=ch[k];
				ch[k]=ch[k-1];
				ch[k-1]=temp;
				count++;
				
				while(k-1>=j){
					k--;
				    str2=str2+ch[k];
				}
				count++;
		}
		return count;
	}

	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		String st = sc.nextLine();

		int step = countStep(st);
		System.out.println(step);

		sc.close();
	}
}
