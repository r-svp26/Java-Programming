package org.thread.in;

public class SuspendThread {

	public static void main(String[] args) {
		
		JoinThread thread=new JoinThread("JSpiders");
		
		thread.start();
		try {
			Thread.sleep(100);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		thread.suspend();
		try {
			Thread.sleep(100);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		
		
	}
}
