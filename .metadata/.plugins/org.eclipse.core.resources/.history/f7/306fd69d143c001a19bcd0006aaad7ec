package com.number.org;

import java.util.Scanner;

public class FibonacciSeries {
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number");
		int num=sc.nextInt();
		int first=0;
		int second=1;
		int third;
		System.out.print(first+" ");
		
		
		for(int i=0;i<num;i++){
			third=first+second;
			first=second;
			second=first;
		}
		
	}

}
