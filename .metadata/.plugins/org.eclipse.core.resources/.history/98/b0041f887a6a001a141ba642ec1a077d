package com.array.in;

public class OddEvenElement {
	public static void main(String[] args) {
		
	
	int[] arr={10,13,25,20,27,36};
	
	int oddCount=0;
	int evenCount=0;
	
	for(int i=0;i<arr.length;i++){
		if(arr[i]%2==0){
			evenCount++;
		}else{
			oddCount++;
		}
	}
	System.out.println("The No. of Even elements: "+evenCount);
}
}
