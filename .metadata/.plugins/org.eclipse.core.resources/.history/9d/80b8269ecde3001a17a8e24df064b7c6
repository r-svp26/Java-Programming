package org.comparator;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;

public class StudentTest {

	public static void main(String[] args) {
		
		List<Student> lst=new ArrayList<Student>();
		
		lst.add(new Student(1,"Ritesh","CS","BTM"));
		lst.add(new Student(4,"Khusi","EC","Delhi"));
		lst.add(new Student(2,"Shyama","IS","Pune"));
		lst.add(new Student(3,"Deepika","BEE","Patna"));
		lst.add(new Student(5,"Radha","CE","Bhopal"));
		
		Collections.sort(lst);
		
	}

}
